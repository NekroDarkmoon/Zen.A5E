    {
      "_id": "hq0FbsvRxXYEphBt",
      "name": "Suggestion (10 Years Old)",
      "type": "spell",
      "img": "icons/svg/item-bag.svg",
      "data": {
        "ability": "",
        "actionOptions": [
          "savingThrow"
        ],
        "activation": {
          "cost": 1,
